{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "components/layout/HeroSection.jsx", "lib/HeroBackground.jsx", "components/layout/Header.jsx", "app/layout.jsx", "components/ui/demo-components-animate-avatar-group.jsx", "lib/utils.js", "components/ui/animated-testimonials.js" , "components/layout/Aboutus.jsx", "components/ServicesSections/ServiesCards.jsx", "app/Services/page.jsx", "app/Contact/page.jsx", "app/Auth/page.jsx", "app/About/page.jsx", "components/ServicesSections/ExpertCardsSection.jsx", "components/ServicesSections/workflow.jsx", "components/ServicesSections/ProcessSection.jsx" ],
  "exclude": ["node_modules"]
}
